<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="com.simplilearn.product">
	<class name="Product">
		<id name="id">
			<generator class="increment" />
		</id>
		<property name="name" type="string" />
		<property name="price" type="int" />
		<property name="quantity" type="int" />
	</class>
</hibernate-mapping>