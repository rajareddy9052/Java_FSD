<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="s" class="com.DemoSpring.Student">
		<property name="name" value="Rajagopal"></property>
		<property name="marks" ref="m"></property>
	</bean>
	<bean id="m" class="com.DemoSpring.Marks">
		<property name="phy" value="95"></property>
		<property name="chem" value="90"></property>
		<property name="math" value="100"></property>
	</bean>
</beans>